{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/icons/pitch_icon.js", "app/(auth)/signin/page.js", "app/(auth)/signup/page.js", "app/component/Breadcrumb.js", "app/layout.js", "components/Breadcrumbs/Breadcrumb.js", "app/(dashboard)/chart/page.js", "app/(dashboard)/settings/page.js", "components/Charts/ChartThree.js", "components/Dashboard/E-commerce.js", "components/Sidebar/index.js", "app/(dashboard)/layout.js", "components/Charts/ChartOne.js", "components/common/Loader/index.js", "app/controllers/story_controller.js", "components/Header/DropdownNotification.js"],
  "exclude": ["node_modules"]
}
